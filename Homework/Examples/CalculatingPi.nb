Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"PiApproximation", "[", "n_", "]"}], ":=", 
   RowBox[{"4.0", "*", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"k", "=", "1"}], "n"], 
     FractionBox[
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"-", "1"}], ")"}], 
       RowBox[{"k", "+", "1"}]], 
      RowBox[{
       RowBox[{"2.", "*", "k"}], "-", "1"}]]}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"ComputePi", "[", 
  RowBox[{"1000", ",", "1"}], "]"}]}], "Input",
