  RowBox[{
   RowBox[{"PiApproximation", "[", "n_", "]"}], ":=", " ", 
   FractionBox[
    SuperscriptBox["99", "2"], 
    RowBox[{"2", 
     SqrtBox["2"], 
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"k", "=", "0"}], "n"], 
      RowBox[{
       FractionBox[
        RowBox[{
         RowBox[{"(", 
          RowBox[{"4.0", "*", "k"}], ")"}], "!"}], 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"k", "!"}], ")"}], "4"]], 
       FractionBox[
        RowBox[{
         RowBox[{"26390", "*", "k"}], "+", "1103"}], 
        SuperscriptBox["396", 
         RowBox[{"4", "*", "k"}]]]}]}]}]]}], ";"}], "\n", 
 RowBox[{"ComputePi", "[", 
