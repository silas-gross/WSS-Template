3dddcd442817"],

Cell[TextData[Cell[BoxData[
 RowBox[{
  RowBox[{
   UnderoverscriptBox[
    RowBox[{" ", "\[Product]"}], 
    RowBox[{"n", "=", "1"}], "\[Infinity]"], 
   RowBox[{"(", 
    RowBox[{
     FractionBox[
      RowBox[{"2", "n"}], 
      RowBox[{
       RowBox[{"2", "n"}], "-", "1"}]], ".", 
     FractionBox[
      RowBox[{"2", "n"}], 
      RowBox[{
       RowBox[{"2", "n"}], "+", "1"}]]}], ")"}]}], " ", "=", " ", 
  FractionBox["\[Pi]", 
   "2"]}]],ExpressionUUID->"92dc5f24-b442-4f63-b08c-b6ad90d17af5"]], "Text",
 CellChangeTimes->{{3.735483932944489*^9, 3.7354839472643557`*^9}, 
   3.735486472511282*^9, {3.735587929314433*^9, 3.735588040172488*^9}, {
   3.7360944038458967`*^9, 3.736094409443921*^9}, 3.736868574101507*^9, 
   3.739291832806913*^9, {3.739291972933859*^9, 3.739292106331347*^9}, {
