abbde02041a1"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"PiApproximation", "[", "n_", "]"}], " ", ":=", 
   RowBox[{
    UnderoverscriptBox["\[Sum]", 
     RowBox[{"k", "=", "0"}], "n"], 
    RowBox[{
     FractionBox["1", 
      SuperscriptBox["16", "k"]], 
     RowBox[{"(", 
      RowBox[{
       FractionBox["4.0", 
        RowBox[{
         RowBox[{"8", "*", "k"}], "+", "1"}]], "-", 
       FractionBox["2.0", 
        RowBox[{
         RowBox[{"8", "*", "k"}], "+", "4"}]], "-", 
       FractionBox["1.0", 
        RowBox[{
         RowBox[{"8", "*", "k"}], "+", "5"}]], "-", 
       FractionBox["1.0", 
        RowBox[{
         RowBox[{"8", "*", "k"}], "+", "6"}]]}], ")"}]}]}]}], ";"}], "\n", 
 RowBox[{"ComputePi", "[", 
  RowBox[{"1000", ",", "3"}], "]"}]}], "Input",
 CellChangeTimes->{
  3.739289711509359*^9},ExpressionUUID->"0927ff9c-fc12-479e-bfa4-\
f82f99aeedd3"],

